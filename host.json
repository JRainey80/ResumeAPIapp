{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Debug",
      "Host.Results": "Information",
      "Function": "Information",
      "Microsoft": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "(4.0.0, 5.0.0)"
  }
}